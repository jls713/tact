	Archiveprefix = {arXiv},
	Author = {{Siegel}, M.~H. and {Dotter}, A. and {Majewski}, S.~R. and {Sarajedini}, A. and {Chaboyer}, B. and {Nidever}, D.~L. and {Anderson}, J. and {Mar{\'{\i}}n-Franch}, A. and {Rosenberg}, A. and {Bedin}, L.~R. and {Aparicio}, A. and {King}, I. and {Piotto}, G. and {Reid}, I.~N.},
	Date-Added = {2012-10-02 10:05:49 +0000},
