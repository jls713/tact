	Doi = {10.1086/176247},
	Eprint = {arXiv:astro-ph/9502005},
	Journal = {\apj},
	Keywords = {GALAXIES: EVOLUTION, GALAXIES: INDIVIDUAL NAME: SAGITTARIUS, GALAXIES: INTERACTIONS, GALAXY: STRUCTURE},
	Month = oct,
	Pages = {598},
	Title = {{The Disruption of the Sagittarius Dwarf Galaxy}},
	Volume = 451,
