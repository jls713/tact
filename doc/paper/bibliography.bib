	Doi = {10.1086/152875},
	Journal = {\apj},
	Keywords = {Astronomical Maps, Galactic Structure, Hydrogen Clouds, Intergalactic Media, Magellanic Clouds, Interstellar Gas, Microwave Emission, Milky Way Galaxy, Radial Velocity, Radio Astronomy, Sky Surveys (Astronomy)},
	Month = jun,
