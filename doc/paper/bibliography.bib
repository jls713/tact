	Journal = {\apj},
	Keywords = {Galaxy: halo, Galaxy: structure, globular clusters: general},
	Month = mar,
	Pages = {1118-1127},
	Title = {{Four New Stellar Debris Streams in the Galactic Halo}},
	Volume = 693,
	Year = 2009,
	Bdsk-Url-1 = {http://dx.doi.org/10.1088/0004-637X/693/2/1118}}

@article{Pryor2010,
	Abstract = {{We have derived a proper motion of Sagittarius using archival data
obtained with the Hubble Space Telescope. The data consist of imaging at
three epochs with a time baseline of about four years in three distinct
fields. The zero point for the proper motion is based on the foreground
Galactic stellar populations along the line of sight. The measured
proper motion in the Galactic coordinate system is ({$\mu$}$_{ell}$,
{$\mu$}$_{ b }$) = (-2.615 {\plusmn} 0.22, 1.87 {\plusmn} 0.19) mas
yr$^{-1}$ and in the equatorial coordinate system is
({$\mu$}$_{alpha}$, {$\mu$}$_{delta}$) = (-2.75 {\plusmn} 0.20,
- 1.65 {\plusmn} 0.22) mas yr$^{-1}$. Removing the contribution of
the motion of the Sun and of the LSR to the measured proper motion
produces a Galactic rest-frame proper motion of ({$\mu$}$^{Grf}$
$_{ell}$, {$\mu$}$^{Grf}$ $_{ b }$) = (-0.82 {\plusmn}
0.22, 1.98 {\plusmn} 0.19) mas yr$^{-1}$ and ({$\mu$}$^{Grf}$
$_{alpha}$, {$\mu$}$^{Grf}$ $_{delta}$) = (-2.14
{\plusmn} 0.20, 0.03 {\plusmn} 0.20) mas yr$^{-1}$. The implied
space velocity with respect to the Galactic center is ({$\Pi$}, {$\Theta$}, Z)
= (141.9 {\plusmn} 6.9, 117 {\plusmn} 29, 238 {\plusmn} 27) km
s$^{-1}$. This velocity implies that the instantaneous orbital
inclination is 67{\deg}, with a 95\% confidence interval of (58{\deg},
79{\deg}). We also present photometry and membership probabilities for
the stars in our sample, which can be used to generate color-magnitude
diagrams for stellar populations selected by proper motion.

Based on observations with NASA/ESA Hubble Space Telescope, obtained at
the Space Telescope Science Institute, which is operated by the
Association of Universities for Research in Astronomy, Inc., under NASA
contract NAS 5-26555.
}},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2010AJ....139..839P},
	Author = {{Pryor}, C. and {Piatek}, S. and {Olszewski}, E.~W.},
	Date-Added = {2012-10-02 10:07:06 +0000},
	Date-Modified = {2012-10-02 10:14:43 +0000},
	Doi = {10.1088/0004-6256/139/3/839},
	Journal = {\aj},
	Keywords = {astrometry, galaxies: dwarf, galaxies: individual: Sagittarius},
	Month = mar,
