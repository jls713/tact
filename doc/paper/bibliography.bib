	Date-Modified = {2012-10-02 10:19:56 +0000},
	Doi = {10.1088/0004-637X/725/2/2290},
	Eprint = {1010.5257},
	Journal = {\apj},
	Keywords = {astrometry, galaxies: dwarf, Galaxy: kinematics and dynamics, Galaxy: stellar content, Galaxy: structure, Local Group, stars: abundances, stars: kinematics and dynamics},
	Month = dec,
